#!/bin/bash

#SBATCH --partition=gpu
#SBATCH --gpus=1
#SBATCH --job-name=ORIG_WHISPER_BASE_ATTNS
#SBATCH --ntasks=1
#SBATCH --time=00:05:00
#SBATCH --output=output/original_experiments/whisper_base_attns_%A.out

module purge
module load 2022
